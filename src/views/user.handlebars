<main>
    <h1>Listado Usuarios</h1>
    <table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Nombre</th>
            <th scope="col">Apellido</th>
            <th scope="col">Edad</th>
            <th scope="col">Email</th>
            <th scope="col">Rol</th>
            <th scope="col">Acciones</th>
        </tr>
    </thead>
    <tbody>
        {{#each users}}
        <tr>
            <th scope="row">{{@index}}</th>
            <td>{{this.first_name}}</td>
            <td>{{this.last_name}}</td>
            <td>{{this.age}}</td>
            <td>{{this.email}}</td>            
            <td>{{this.role}}</td>
            <td>
                <button class="btn btn-primary btn-sm" id="edit">Editar</button>
                <button class="btn btn-danger btn-sm" id="del">Eliminar</button>
            </td>
        </tr>
       {{/each}}
    </tbody>
</table>

</main>


